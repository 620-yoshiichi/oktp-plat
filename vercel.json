{
 "framework": "nextjs",
 "installCommand": "npm install",
 "devCommand": "next dev",
 "buildCommand": "npx prisma generate && next build --no-lint",
 "cleanUrls": true,
 "regions": [
  "sin1"
 ],
 "functions": {
  "src/app/api/**": {
   "memory": 3009
  },
  "src/app/(apps)/*/api/**": {
   "memory": 3009
  }
 },
 "trailingSlash": false,
 "crons": [
  {
   "path": "/newCar/api/cron/orderUpsert",
   "schedule": "0 21,3 * * *"
  },
  {
   "path": "/newCar/api/cron/tenpoTsuikoUpsert",
   "schedule": "0 22-23,0-10 * * *"
  },
  {
   "path": "/newCar/api/cron/fetchSeisanYoteiDiff",
   "schedule": "15 1 * * *"
  },
  {
   "path": "/newCar/api/cron/notifySeisanYoteiDiff",
   "schedule": "0,30 * * * *"
  },
  {
   "path": "/newCar/api/cron/aggregateProgress",
   "schedule": "0 11 28-31 * *"
  },
  {
   "path": "/ucar/api/cron/oldCars/deleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/ucar/api/cron/zaiko/deleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/ucar/api/cron/aisatei/deleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/ucar/api/cron/upass/deleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/ucar/api/cron/juchuShitadoriDb/deleteAndCreate",
   "schedule": "0 22 * * *"
  }
 ]
}
