{
 "framework": "nextjs",
 "installCommand": "npm install",
 "devCommand": "next dev",
 "buildCommand": "npx prisma generate && next build --no-lint",
 "cleanUrls": true,
 "regions": [
  "sin1"
 ],
 "functions": {
  "src/app/api/**": {
   "memory": 3009
  },
  "src/app/(apps)/*/api/**": {
   "memory": 3009
  }
 },
 "trailingSlash": false,
 "crons": [
  {
   "path": "/api/cron/execute/aggregateProgress",
   "schedule": "0 11 28-31 * *"
  },
  {
   "path": "/api/cron/execute/aisateiDeleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/api/cron/execute/fetchSeisanYoteiDiff",
   "schedule": "15 1 * * *"
  },
  {
   "path": "/api/cron/execute/juchuShitadoriDbDeleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/api/cron/execute/notifySeisanYoteiDiff",
   "schedule": "0,30 * * * *"
  },
  {
   "path": "/api/cron/execute/oldCarsDeleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/api/cron/execute/orderUpsert",
   "schedule": "0 21,3 * * *"
  },
  {
   "path": "/api/cron/execute/tenpoTsuikoUpsert",
   "schedule": "0 22-23,0-10 * * *"
  },
  {
   "path": "/api/cron/execute/upassDeleteAndCreate",
   "schedule": "0 22 * * *"
  },
  {
   "path": "/api/cron/execute/zaikoDeleteAndCreate",
   "schedule": "0 22 * * *"
  }
 ]
}
